group by  a.provider_id,a.activity_date ) as bb
on aa.provider_id =bb.provider_id
and aa.activity_date=bb.activity_date
and convert(time,aa.schedule_time)=bb.schedule_time
and aa.activity_start_time is null and aa.activity_end_time is null

) as t2 

INNER JOIN [appsql-3\comcareprod].[comcareprod].dbo.person as t3

ON t2.Provider_ID=t3.Person_ID 


JOIN [appsql-3\comcareprod].[comcareprod].dbo.Provider_Contract as t4
 
ON t2.Provider_ID=t4.Provider_ID 

JOIN [appsql-3\comcareprod].[comcareprod].dbo.Organisation as t5

ON t4.Organisation_ID=t5.Organisation_ID 

JOIN [appsql-3\comcareprod].[comcareprod].dbo.Provider_Classification as t6

ON t4.Provider_Class_Code=t6.Provider_Class_Code

where t5.Organisation_Name = @RAC_Site  and t6.Description in(@Provider_Classification) and t2.SPPID&lt;&gt;0 and t2.sppid is not null

Group by t3.Person_ID, t5.Organisation_Name, t3.Last_Name, t3.given_Names, t6.Description
order by t3.Last_Name</CommandText>
